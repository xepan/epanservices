div(id="{$_name}" class="{$class} agency-navbar" style="{$style}")
	nav.navbar
		.container-fluid
			.navbar-header
				button.navbar-toggle.collapsed(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded='false')
					span.sr-only Toggle navigation
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.navbar-brand(href='dashboard')
					img(src='websites/www/www/images/logo.png')
			#bs-example-navbar-collapse-1.collapse.navbar-collapse
				ul.nav.navbar-nav
					{rows}{row}
					li(data-page='{$key}' class="{$active_menu}")
						a(href='{$key}') {$name}
					{/}{/}
				ul.nav.navbar-nav.navbar-right
					li.bg.bg-info(style="border-radius:5px 0px 0px 5px;") 
						a(href="#" style="padding:10px;float:left;" class="text-center") {$customer_name}<br/>{$user_name}
						a(href="#" style="padding:10px; box-shadow:inset 0px 0px 1px 2px #ccc;border-radius:50%;float:left;" title="{$customer_name}" class="text-center")
							img(src="{$customer_dp}" style="max-height:60px;max-width:40px;height:40px;" class="ds-dp")
						a(style="padding:10px;float:left;" href="logout")
							div.btn.btn-danger()
								i.glyphicon.glyphicon-off
								|&nbsp;Logout
	{$Content}